{
  "name": "fuhrpark-manager",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.6.0",
  "workspaces": [
    "apps/web",
    "apps/api"
  ],
  "scripts": {
    "web:dev": "npm run dev --workspace web",
    "web:build": "npm run build --workspace web",
    "web:lint": "npm run lint --workspace web",
    "api:dev": "npm run start:dev --workspace api",
    "api:build": "npm run build --workspace api",
    "api:lint": "npm run lint --workspace api",
    "api:test": "npm run test --workspace api",
    "web:start": "npm run start --workspace web",
    "api:start": "npm run start --workspace api"
  }
}
